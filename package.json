    "@babel/preset-react": "^7.12.7",
    "bili": "^5.0.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "parcel-bundler": "^1.12.4",
    "eslint-plugin-react": "^7.21.5",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.2.0"
