  "description": "A clean & simple modal component for React",
  "main": "dist/index.js",
  "source": "dist/umd/react-standard-modal.min.js",
  "scripts": {
    "dev": "parcel serve example/src/index.html",
    "format": "prettier \"src/**/*.{js,json}\" --write",
